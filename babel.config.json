{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        [
            "module-resolver",
            {
                "root": [
                    "."
                ],
                "alias": {
                    "@support": "./support",
                    "@utils": "./support/utils",
                    "@files": "./files"
                }
            }
        ]
    ]
}